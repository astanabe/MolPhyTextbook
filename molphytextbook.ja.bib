@Article{Hrdy2004,
  Title                    = {Trichomonas hydrogenosomes contain the NADH dehydrogenase module of mitochondrial complex I},
  Author                   = {Ivan Hrdy and Robert P Hirt and Pavel Dolezal and Lucie Bardonov\'{a} and Peter G Foster and Jan Tachezy and T. Martin Embley},
  Journal                  = {Nature},
