  Institution              = {Bioinformatics Institute, University of Auckland, Auckland, New Zealand. alexei@cs.auckland.ac.nz},
  Keywords                 = {Bayes Theorem; Computational Biology; Computer Simulation; Evolution, Molecular; Models, Genetic; Models, Statistical; Phylogeny; Sequence Analysis, DNA; Software},
